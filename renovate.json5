{
  extends: [
    "config:recommended",
    "github>suzuki-shunsuke/renovate-config#2.2.0",
    "github>aquaproj/aqua-renovate-config#2.0.0",
    "github>suzuki-shunsuke/renovate-config:nolimit#2.2.0",
    "github>aquaproj/aqua-renovate-config:file#2.0.0(CONTRIBUTING\\.md)",
    "github>aquaproj/aqua-renovate-config:installer-script#2.0.0(docker/Dockerfile)",
    "github>suzuki-shunsuke/renovate-config:action-go-version#2.2.0",
  ],
  regexManagers: [
    {
      fileMatch: ["README.md"],
      matchStrings: [
        "ref: (?<currentValue>.*?) # renovate: depName=(?<depName>.*)",
      ],
      datasourceTemplate: "github-releases",
    },
  ],
}
