{
  extends: [
    "config:base",
    "github>suzuki-shunsuke/renovate-config#2.1.0",
    "github>aquaproj/aqua-renovate-config#1.7.0",
    "github>suzuki-shunsuke/renovate-config:nolimit#2.1.0",
    "github>aquaproj/aqua-renovate-config:file#1.7.0(CONTRIBUTING\\.md)",
    "github>aquaproj/aqua-renovate-config:installer-script#1.8.0(Earthfile)",
  ],
  regexManagers: [
    {
      fileMatch: ["README.md"],
      matchStrings: [
        "ref: (?<currentValue>.*?) # renovate: depName=(?<depName>.*)",
      ],
      datasourceTemplate: "github-releases",
    },
  ],
}
