{
  extends: [
    "config:base",
    "helpers:pinGitHubActionDigests",
    "github>aquaproj/aqua-renovate-config#1.3.0",
    "github>aquaproj/aqua-renovate-config:file#1.3.0(CONTRIBUTING\\.md)",
    "github>aquaproj/aqua-renovate-config:file#1.3.0(aqua-all\\.yaml)",
    "github>aquaproj/aqua-renovate-config:file#1.3.0(aqua-ci\\.yaml)",
    "github>aquaproj/aqua-renovate-config:file#1.3.0(pkgs/.*/pkg\\.yaml)",
  ],
  automerge: true,
  platformAutomerge: true,
  prHourlyLimit: 0,
  prConcurrentLimit: 0,
  branchConcurrentLimit: 0,
  regexManagers: [
    {
      fileMatch: ["README.md"],
      matchStrings: [
        "ref: (?<currentValue>.*?) # renovate: depName=(?<depName>.*)",
      ],
      datasourceTemplate: "github-releases",
    },
    {
      fileMatch: ["^\\.github/workflows/.*\\.ya?ml$"],
      depNameTemplate: "sigstore/cosign",
      datasourceTemplate: "github-releases",
      matchStrings: [
        "cosign-release: (?<currentValue>.*) +# renovate: depName=sigstore/cosign",
      ],
    },
  ],
}
