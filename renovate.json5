{
  extends: [
    "config:base",
    "github>clienv/clienv-renovate-config#1.1.0",
    "github>clienv/clienv-renovate-config:file#1.1.0(CONTRIBUTING\\.md)",
    "github>clienv/clienv-renovate-config:file#1.1.0(clienv-all\\.yaml)",
    "github>clienv/clienv-renovate-config:file#1.1.0(clienv-ci\\.yaml)",
    "github>clienv/clienv-renovate-config:file#1.1.0(clienv-dev\\.yaml)",
    "github>clienv/clienv-renovate-config:file#1.1.0(pkgs/.*\\.yaml)",
  ],
  automerge: true,
  platformAutomerge: true,
  prHourlyLimit: 0,
  prConcurrentLimit: 0,
  branchConcurrentLimit: 0,
  regexManagers: [
    {
      fileMatch: ["README.md"],
      matchStrings: [
        "ref: (?<currentValue>.*?) # renovate: depName=(?<depName>.*)",
      ],
      datasourceTemplate: "github-releases",
    },
  ],
}
